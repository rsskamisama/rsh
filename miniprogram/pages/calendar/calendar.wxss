/* pages/calendar/calendar.wxss */
page{height:100%}
.date{
  display: flex;
  flex-direction: row;
  padding-left: 20rpx;
  padding-right: 20rpx;
  justify-content: space-around;
}
.ul { 
  padding: 20rpx;
  height: 650rpx;
}
.unchecked_li {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  list-style-type: none;
  float: left;
  width: 100rpx;
  height: 90rpx;
  padding-top:10rpx ;
}
.checked_li{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  list-style-type: none;
  float: left;
  width: 100rpx;
  height: 90rpx;
  color: white;
  background-color: CornflowerBlue;
  padding-top:10rpx ;
  border-radius: 15rpx;
}
.white{
  display: block;
  padding-top: 50rpx;
  width: 100%;
  height: 100%;
  background-color: white;
}
.hidden{
  visibility: hidden;
}
.center{
  text-align:center;
  padding-bottom: 20rpx;
}
.unchecked_yuan{
  text-align:center;
  color:CornflowerBlue ;
  font-size: x-small;
}
.checked_yuan{
  text-align:center;
  color:white ;
  font-size: x-small;
}